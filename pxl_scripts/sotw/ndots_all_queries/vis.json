{
  "variables": [
    {
      "name": "start",
      "type": "PX_STRING",
      "description": "The start time of the window in time units before now.",
      "defaultValue": "-3h"
    },
    {
      "name": "hostname",
      "type": "PX_STRING",
      "description": "The hostname used to filter the DNS requests by.",
      "defaultValue": "news.ycombinator.com"
    }
  ],
  "globalFuncs": [
    {
      "outputName": "fqdns",
      "func": {
        "name": "fqdns",
        "args": [
          {
            "name": "start",
            "variable": "start"
          },
          {
            "name": "hostname",
            "variable": "hostname"
          }
        ]
      }
    }
  ],
  "widgets": []
}